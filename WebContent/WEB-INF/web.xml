<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>StockExchange</display-name>
  <welcome-file-list>
    <welcome-file>/jsp/HomePage.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>GetTickerData</display-name>
    <servlet-name>GetTickerData</servlet-name>
    <servlet-class>com.stockexchange.restapi.GetTickerData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetTickerData</servlet-name>
    <url-pattern>/GetTickerData</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetCompanyData</display-name>
    <servlet-name>GetCompanyData</servlet-name>
    <servlet-class>com.stockexchange.restapi.GetCompanyData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetCompanyData</servlet-name>
    <url-pattern>/GetCompanyData</url-pattern>
  </servlet-mapping>
</web-app>